{
    "manifest_version": 3,
    "name": "PDF Dark Mode",
    "version": "1.0",
    "description": "Enable dark mode for PDFs in Microsoft Edge.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  